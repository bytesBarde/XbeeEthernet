net: "models/psdb/resnet50/pretrain.prototxt"

solver_type: SGD

test_iter: 15
test_interval: 100
test_initialization: true

base_lr: 0.001
lr_policy: "step"
gamma: 0.1
stepsize: 5000
max_iter: 8000

display: 20
average_loss: 100

momentum: 0.9
weight_decay: 0.0005

snapshot: 1000
snapshot_prefix: "output/psdb_pretrain/resnet50"
solver_mode: GPU

device_id: [0,1,2,3,4,5,6,7]
